<?xml version="1.0"?>
<block>
  <name>FCCH burst tagger</name>
  <key>gsm_fcch_burst_tagger</key>
  <import>import grgsm</import>
  <make>grgsm.fcch_burst_tagger($OSR)</make>
  <param>
    <name>OSR</name>
    <key>OSR</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  
  <sink>
    <name>threshold</name>
    <type>float</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
